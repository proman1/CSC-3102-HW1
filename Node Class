public class Node 
{
    int value;
    Node left;
    Node right;
    Node parent;
    
    // Constructor
    Node()
    {
        value = -1;
        left = null;
        right = null;
        parent = null;        
    }
    
    Node(int num, Node leftChild, Node rightChild, Node father)
    {
        value = num;
        left = leftChild;
        right = rightChild;
        parent = father;        
    }  
}
